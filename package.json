{
    "name": "directus-extension-mux-video",
    "description": "Adds an interface to upload videos to Mux and manage them in Directus",
    "icon": "extension",
    "version": "1.0.0",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-extension-bundle",
        "directus-extension-interface",
        "directus-extension-endpoint",
        "mux",
        "directus-extension-mux"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "directus:extension": {
        "type": "bundle",
        "path": {
            "app": "dist/app.js",
            "api": "dist/api.js"
        },
        "entries": [
            {
                "type": "endpoint",
                "name": "muxgenerator",
                "source": "src/muxgenerator/index.ts"
            },
            {
                "type": "interface",
                "name": "muxvideo",
                "source": "src/muxvideo/index.ts"
            }
        ],
        "host": "^10.10.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link",
        "add": "directus-extension add"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "11.0.1",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.11.0",
        "vue": "^3.4.21"
    },
    "dependencies": {
        "@mux/mux-node": "^9.0.1",
        "@mux/mux-player": "^2.3.3",
        "@mux/mux-uploader": "^1.0.0-beta.15",
        "@mux/mux-video": "^0.17.3",
        "axios": "^1.6.8",
        "body-parser": "^1.20.3",
        "express": "^4.21.2",
        "raw-body": "^3.0.0"
    }
}
